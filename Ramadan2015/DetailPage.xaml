<Page
    x:Class="Ramadan2015.DetailPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Ramadan2015"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

	<Grid>
		<StackPanel>
			<TextBlock x:Name="Serial" FontSize="20" ></TextBlock>
			<TextBlock x:Name="Date" FontSize="20" ></TextBlock>
			<TextBlock x:Name="Sehri" FontSize="20" ></TextBlock>
			<TextBlock x:Name="Iftar" FontSize="20" ></TextBlock>
			<TextBlock x:Name="timetosehrir" FontSize="20" ></TextBlock>
			<TextBlock x:Name="timetoifter" FontSize="20" ></TextBlock>
		</StackPanel>


	</Grid>
</Page>
